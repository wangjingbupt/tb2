<script type="text/javascript">

isLogin=false;
$j.ajax({
	url: ajax_url+"/customer/headerAjax.html?decorator=null&timeStamp="+new Date().getTime(),
	dataType: "json",
	success: function(responseText) {
		var rJSON=responseText;
		var urlKey=(!!rJSON.customer.urlKey)?rJSON.customer.urlKey:("people/"+rJSON.customer.customerId);
		var customerId=rJSON.customer.customerId;
		var nickname=rJSON.customer.nickname;
		var headMyDiv="";
		var customerHeader="";
		var miniCart="";
		var numTotal=rJSON.cart.productSize;
		var subTotal=rJSON.cart.subtotal;
		isLogin=(!!rJSON.status.login);
		
		//topper
		customerHeader='<li class="noses"><a href="javascript:toLogin()">登录</a></li>';
		customerHeader+='<li class="noses"><a href="'+ajax_url+'/register.html">注册</a></li>';
		
		
		
		if(isLogin){
			//header
			headMyDiv='<a class="myPage" href="'+ajax_url+'/'+urlKey+'/likes/styles">我的喜欢</a><a class="myPage" href="'+ajax_url+'/'+urlKey+'/collections">我的图集</a><a class="myPage" href="'+ajax_url+'/'+urlKey+'/styles">我的单品</a>';
			//topper
			customerHeader='<li class="user_name"><strong>'+nickname+'</strong></li>';
			customerHeader+='<li class="myses"><a href="javascript:void(0)" class="toggle">消息<span class="icon">&nbsp;</span></a>';
			customerHeader+='<div class="toggleDiv"><ul class="viewTotal">';
			customerHeader+='<li class="viewReviewNewCount"><a href="'+ajax_url+'/message/comments.html">查看评论&nbsp;<span class="numc"></span></a></li>';
			customerHeader+='<li class="viewAtMeNewCount"><a href="'+ajax_url+'/message/mentions.html">查看@我&nbsp;<span class="numc"></span></a></li>';
			customerHeader+='<li class="viewSysMsgNewCount"><a href="'+ajax_url+'/message/systemMsg.html">查看通知&nbsp;<span class="numc"></span></a></li>';
			customerHeader+='</ul></div></li>';
			customerHeader+='<li class="myses"><a href="javascript:void(0)" class="toggle">账号<span class="icon">&nbsp;</span></a>';
			customerHeader+='<div class="toggleDiv"><ul class="accountSet">';
			customerHeader+='<li class="my_accountSet"><a href="'+ajax_url+'/customer/accountInfo.html">账户设置</a></li>';
			customerHeader+='<li class="my_orders"><a href="'+ajax_url+'/customer/myOrderList.html">我的订单</a></li>';
			customerHeader+='<li class="my_invite"><a href="'+ajax_url+'/customer/customerInvite.html">邀请好友</a></li>';
			customerHeader+='<li class="my_giftCard"><a href="'+ajax_url+'/customer/gift-card-balance.html">充值卡</a></li>';
			customerHeader+='</ul></div></li>';
			customerHeader+='<li class="myses"><a href="'+ajax_url+'/logout.html">退出</a></li>';
		}
		//miniCart
		var tempNm,nums=0;
		if(numTotal=="0"){
			tempNm='';
			miniCart='<div class="miniCartContent empty">';
			miniCart+='购物车中暂无商品！';
			miniCart+='<div class="viewCart"><a class="btn" href="'+ajax_url+'/cart/showCart.html"><strong><em></em>查看购物车</strong><span></span></a></div>';
			miniCart+='</div>';
		}else{
			tempNm='<span class="miniCartNum">('+numTotal+')</span>';
			miniCart='<div class="miniCartContent">';
			miniCart+='<table border="0" cellspacing="0" cellpadding="0">';
			var miniCartNum = 5;
			if(numTotal<5){
				miniCartNum = numTotal;
			}
			for (var m=0;m<miniCartNum;m++){
				miniCart+='<tr>';
				miniCart+='<td width="45" valign="top"><a href="'+rJSON.cart.products[m].productHref+'" target="_blank"><img src="'+rJSON.cart.products[m].imgSrc+'" width="45" height="45" border="0" /></a></td>';
				miniCart+='<td valign="top" align="left" class="title"><a href="'+rJSON.cart.products[m].productHref+'" target="_blank">'+rJSON.cart.products[m].productName+'</a><br/>数量：'+rJSON.cart.products[m].tradeNumber+'</td>';
				if(rJSON.cart.products[m].isBind==0){
					miniCart+='<td width="80" valign="top"><span class="price"><span class="offShelf price">已下架</span><span class="totalPrice price" style="display:none;">&nbsp;</span></span></td>';
				}else{
					miniCart+='<td width="80" valign="top"><span class="price">'+rJSON.cart.products[m].price+'</span></td>';
				}
				miniCart+='</tr>';
				nums+=parseInt(rJSON.cart.products[m].tradeNumber);
			}
			miniCart+='</table>';
			miniCart+='<div class="total">';
			if(numTotal>5){
				miniCart+='<em class="left">剩余'+(numTotal-5)+'种商品未显示</em>';
			}
			miniCart+='<em class="right">总金额：<span class="price">'+subTotal+'</span></em></div>';
			miniCart+='<div class="viewCart"><a href="'+ajax_url+'/cart/showCart.html" class="btn"><strong><em></em>查看购物车</strong><span></span></a></div>';
			miniCart+='</div>';					
		}
		miniCart0='<ul><li><a href="'+ajax_url+'/cart/showCart.html" class="toggle"><em>购物车</em>'+tempNm+'</a><div class="toggleDiv">';
		miniCart=miniCart0+miniCart+'</div></li></ul>';
		
		//add header
		$j("#headMyDiv").html(headMyDiv);
		//add topper
		$j("#customerHeader").html(customerHeader);
		//add miniCart
		$j("#miniCart").html(miniCart);
		
		$j(".nav_top .toggle").parent().hover(function() {
				$j(this).find(".toggleDiv").show();
				$j(".miniCartNum").css("text-decoration","underline");
			},
			function() {
				$j(this).find(".toggleDiv").hide();
				$j(".miniCartNum").css("text-decoration","none");
		})
	}
})
	

</script>	</div>
