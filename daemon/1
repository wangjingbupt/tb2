<script type="text/javascript">
/*头部导航展开*/
$j(document).ready(function() {				
	var hoverEls = $j(".nav_sec > li");
	for (var i = 0; i < hoverEls.length; i++) {
		$j(hoverEls[i]).find(".submenu_bg").height($j(hoverEls[i]).find(".submenu_wrapper").height() - 1); //-1 for ie6
		$j(hoverEls[i]).find(".submenu_bg").width($j(hoverEls[i]).find(".submenu_wrapper").width());
		$j(hoverEls[i]).find(".submenu_bg").fadeTo("fast", 0.9);
	}
	$j(".nav_sec > li").hover(function() {
		$j(this).find(".submenu_wrapper").show();		
	},
	function() {
		$j(this).find(".submenu_wrapper").hide();
	})
});
</script>
