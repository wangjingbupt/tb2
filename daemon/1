	<span id="prepagenum" keyTo="http://dongdamen.yiss.com/styles/1694555.html">上一页</span>&nbsp;<img src="http://dimg.yissimg.com/themes/front/yiss/images/keyTo_01.png" border="0"/>&nbsp;<strong>使用键盘浏览</strong>&nbsp;<img src="http://dimg.yissimg.com/themes/front/yiss/images/keyTo_02.png" border="0"/>&nbsp;<span id="nextpagenum" keyTo="http://dongdamen.yiss.com/styles/1694553.html">下一页</span>
	<script>
		var keyTime=2000;//限制间隔时间1分钟
		var iskLock = true;  //定义全局变量，判断是否可以键盘翻页
		var isInTextarea = false;  //定义全局变量，判断是否光标在输入框内
		$j(function(){
			$j("textarea#textarea_reply").bind({
			  focus: function() {
				isInTextarea=true;
			  },
			  blur: function() {
				isInTextarea=false;
			  }
			});
		})
		document.onkeyup = function(event){
           if(iskLock && !isInTextarea){
//		   		console.log(000)
				var isie = document.all ? true : false;
				var keycode = isie ? window.event.keyCode : event.keyCode;
				if (0 < $j("#prepagenum").length && keycode == 37) {
					if(!!$j("#prepagenum").attr("keyTo")){
//						console.log(1)
						window.location.href =$j("#prepagenum").attr("keyTo");
						iskLock = false;
					}
				}
				if (0 <  $j("#nextpagenum").length && keycode == 39) {
					if(!!$j("#nextpagenum").attr("keyTo")){
//						console.log(2)
						window.location.href = $j("#nextpagenum").attr("keyTo");
						iskLock = false;
					}
				}
				setTimeout(function (){
				   iskLock = true;
				},keyTime);//超过定义时间后才可以继续翻页
		   }
    
        };
		
  var browser={
    versions:function(){ 
           var u = navigator.userAgent, app = navigator.appVersion; 
           return {//移动终端浏览器版本信息 
                trident: u.indexOf('Trident') > -1, //IE内核
                presto: u.indexOf('Presto') > -1, //opera内核
                webKit: u.indexOf('AppleWebKit') > -1, //苹果、谷歌内核
                gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1, //火狐内核
                mobile: !!u.match(/AppleWebKit.*Mobile.*/), //是否为移动终端
                ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
                android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, //android终端或者uc浏览器
                iPhone: u.indexOf('iPhone') > -1 , //是否为iPhone或者QQHD浏览器
                iPad: u.indexOf('iPad') > -1, //是否iPad
                webApp: u.indexOf('Safari') == -1 //是否web应该程序，没有头部与底部
            };
         }(),
         language:(navigator.browserLanguage || navigator.language).toLowerCase()
	} 
	//滑动
	function slide() {
		var mDocument = document.getElementById("matchItemForm");
		var m = Math, 
			isTouchPad = (/hp-tablet/gi).test(navigator.appVersion), 
			hasTouch = 'ontouchstart' in window && !isTouchPad,
			START_EV = hasTouch ? 'touchstart' : 'mousedown',
			MOVE_EV = hasTouch ? 'touchmove' : 'mousemove',
			END_EV = hasTouch ? 'touchend' : 'mouseup',
			CANCEL_EV = hasTouch ? 'touchcancel' : 'mouseup';
		var pointBeginX=0,pointEndX=0;//移动起始终止位置及方向
		mDocument.addEventListener(START_EV, function (e) {
			pointBeginX = hasTouch ? e.touches[0] : e;
		}, false);
		mDocument.addEventListener(END_EV, function (e) {
			pointEndX = hasTouch ? e.changedTouches[0] : e;
				
			var tnum=Number(pointBeginX.pageX)-Number(pointEndX.pageX)
			if (!!$j("#prepagenum").attr("keyTo")) {
				if(tnum-230>0){//向左滑动，220是预留值
					window.location.href = $j("#prepagenum").attr("keyTo");
				}
			}
			if (!!$j("#nextpagenum").attr("keyTo")) {
				if (tnum+230<0){//向左滑动
					window.location.href = $j("#nextpagenum").attr("keyTo");
				}
			}

		}, false);
	}
	//判断是否是移动端。
	var mobileFlag=browser.versions.mobile||browser.versions.ios||browser.versions.android||browser.versions.iPhone||browser.versions.iPad;
	if(mobileFlag){
		var prePage=$j("#prepagenum").attr("keyTo");
		var nextPage=$j("#nextpagenum").attr("keyTo");
		if (!!prePage) {
			$j("#prepagenum").bind("click",function(){
				window.location.href = prePage;
			})
		}
		if (!!nextPage) {
			$j("#nextpagenum").bind("click",function(){
				window.location.href = nextPage;
			})
		}
		slide();

	}
	</script>
